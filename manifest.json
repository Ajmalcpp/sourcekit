{
  "name": "ChromePad - Text Editor Inside Chrome",
  "version": "0.1",
  "incognito": "split",
  "icons": { "128": "icon.png" },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background_page": "background.html",
  "permissions": ["tabs",
    "http://api.dropbox.com/",
    "https://api.dropbox.com/",
    "http://api-content.dropbox.com/",
    "https://api-content.dropbox.com/",
    "http://api.getdropbox.com/"
  ]
}