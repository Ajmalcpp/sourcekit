{
  "name": "SourceKit - Text Editor Inside Chrome",
  "version": "0.3.5",
  "incognito": "split",
  "icons": { "128": "icon.png" },
  "app": {
    "launch": {
      "local_path": "application.html"
    }
  },
  "options_page": "options.html",
  "background_page": "background.html",
  "permissions": ["tabs",
    "notifications",
    "http://*.dropbox.com/",
    "https://*.dropbox.com/",
    "https://*.getdropbox.com/",
    "http://*.getdropbox.com/"
  ]
}
