{
  "name": "SourceKit - Text Editor Inside Chrome",
  "version": "0.6.0",
  "incognito": "split",
  "icons": { 
    "128": "icons/icon_128.png",
    "16": "icons/icon_16.png"
  },
  "app": {
    "launch": {
      "local_path": "resources/application.html"
    }
  },
  "options_page": "resources/options.html",
  "background_page": "resources/background.html",
  "permissions": [
	"tabs",
    "notifications",
    "http://*.dropbox.com/",
    "https://*.dropbox.com/"
  ]
}
